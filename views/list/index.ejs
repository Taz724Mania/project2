<!DOCTYPE html>
<html lang="en">
  <%- include("./cssjs.ejs") %>
  <body class="mainPage">
    

    <main >
      <div>
        <article class="indexList">

            <% for (let i = 0; i < list.length; ++i) { %>

              <a class="indexHyperlink" href="/list/<%= list[i]._id %>">

                <h1 class="items"><%= list[i].item %></h1> </a>

                <h3 class="store"><%= list[i].location %></h3>

                <h3 class="quantity"><%= list[i].quantity %></h3>

                <h3 class="cost"><%= list[i].cost %></h3>
       
            <% } %>

        </article>
      </div>

      <div class="indexButton">

        <a  href="/list/new"><button class="Button1">Add an Item</button></a>
        
      </div>

    </main>
  </body>
</html>